generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x"]  // Inclua os bin√°rios corretos
}


datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Item {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  image     String
  category  String
  float     String?
  price     Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
